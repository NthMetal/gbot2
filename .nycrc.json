{
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "**/*.spec.ts"
    ],
    "extension": [
        ".ts",
        ".tsx"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "text-summary",
        "html"
    ],
    "sourceMap": true,
    "instrument": true
}