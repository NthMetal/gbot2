{
    "all": true,
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "**/*.spec.ts",
        "src/bot.ts"
    ],
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "text-summary",
        "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true,
    "lines": "100",
    "functions": "100",
    "branches": "100",
    "statements": "100"
}